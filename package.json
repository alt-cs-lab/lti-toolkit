{
  "name": "lti-toolkit",
  "version": "0.0.15",
  "type": "module",
  "description": "LTI Toolkit for ALT+CS Web Applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/alt-cs-lab/lti-toolkit"
  },
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint --fix .",
    "format": "npx prettier . --write",
    "test": "mocha --require test/hooks.js --recursive --parallel --timeout 10000 --exit",
    "cov": "c8 --reporter=html --reporter=text --report-dir ./docs/static/coverage mocha --require test/hooks.js --recursive --parallel --timeout 10000 --exit",
    "doc": "jsdoc -c .jsdoc.json",
    "preversion": "npm run cov && npm run lint && npm run format && npm run doc",
    "version": "git add -A docs/static/coverage docs/static/jsdoc",
    "postversion": "git push && git push --tags"
  },
  "author": "K-State ALT+CS Lab",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "ky": "^1.10.0",
    "nanoid": "^5.1.6",
    "nunjucks": "^3.2.4",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "umzug": "^3.8.2",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "c8": "^10.1.3",
    "chai": "^6.0.1",
    "chai-json-schema-ajv": "^5.2.4",
    "chai-shallow-deep-equal": "^1.4.6",
    "eslint": "^9.36.0",
    "jsdoc": "^4.0.4",
    "mocha": "^11.7.2",
    "nodemon": "^3.1.10",
    "oauth-sign": "^0.9.0",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
